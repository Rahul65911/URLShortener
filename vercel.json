{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "index.js": {
            "includeFiles": ["node_modules/geoip-lite/data/**/*"]
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}
